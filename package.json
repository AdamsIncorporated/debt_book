        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^9.2.1",
        "css-loader": "^6.8.1",
        "eslint": "^8.47.0",
        "eslint-plugin-react": "^7.33.1",
        "html-webpack-plugin": "^5.5.1",
        "source-map-loader": "^4.0.0",
